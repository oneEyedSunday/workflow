{
  "name": "workflow",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish:ghpages": "ng build --prod --base-href '/workflow/' && npx ngh --dir=../ui"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --fix",
      "pre-push": "ng test --watch=false --code-coverage=true --source-map && npm run build"
    }
  },
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "feather-icons": "^4.22.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "ng2-dragula": "^2.1.1",
    "ngx-quill": "^7.1.0",
    "ngx-toastr": "^10.0.4",
    "quill": "^1.3.6",
    "quill-emoji": "^0.1.7",
    "rxjs": "~6.5.2",
    "time-ago-pipe": "^1.3.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
